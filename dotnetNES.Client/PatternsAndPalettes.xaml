<Window x:Class="dotnetNES.Client.PatternsAndPalettes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PatternsAndPalettes" Height="492.5" Width="586"
        DataContext="{Binding PatternsAndPalettes, Source={StaticResource Locator}}">
    <Grid Name="MainGrid">
        <Image HorizontalAlignment="Left" Height="256" Margin="19,41,0,0" VerticalAlignment="Top" Width="256" Source="{Binding PatternTable0 }"/>
        <Image HorizontalAlignment="Left" Height="256" Margin="307,41,0,0" VerticalAlignment="Top" Width="256" Source ="{Binding PatternTable1 }"/>
        <Label Content="Palettes" HorizontalAlignment="Left" Margin="19,15,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.263,0.462"/>
    </Grid>
</Window>
