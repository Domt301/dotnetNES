<Window x:Class="dotnetNES.Client.NameTables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NameTables" Height="540" Width="552" 
        DataContext="{Binding NameTables, Source={StaticResource Locator}}"
        Background="LightGray">
    <Grid>
        <Image HorizontalAlignment="Left" Height="480" Margin="10,10,0,0" VerticalAlignment="Top" Width="512" Source="{Binding NameTable }" RenderOptions.BitmapScalingMode="NearestNeighbor" />
    </Grid>
</Window>
